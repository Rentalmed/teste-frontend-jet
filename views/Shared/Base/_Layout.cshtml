<!DOCTYPE html>
<html lang="pt-br">
<head>   

    <meta name="google-site-verification" content="jRMULVVWtx86NgHvy98CKm3ncSItqoY4iM1a3FJmb34" />

    <meta name="robots" content="INDEX,FOLLOW" />

    <meta name="DC.title" content="RentalMed" />

    <meta name="language" content="pt-br"/>

    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />

    <meta property="og:url" content="https://www.rentalmed.com.br">

    <meta property="og:image" content="imagem da publicação, colocar logo rental">

    <meta property="og:image:type" content="image/jpeg">

    <meta property="og:type" content="website">

    <meta property="og:title" content="RentalMed">

    <meta property="og:description" content=" Na RentalMed você encontra o que há de melhor em aparelhos de Estética e Fisioterapia 
    em até 48x, além de dermocosméticos e cursos especializados. Também oferecemos locações de aparelhos em Santa Catarina. ">
    
    <meta name="keywords" content=" aparelho de estética, móveis para clínicas, aparelhos de estética, equipamentos para estética, aparelho de fisioterapia,
    aparelhos de fisioterapia, equipamentos para fisioterapia, eletroterapia, odontologia, curso de estética, curso estetica paliativa, metodo peronsalize,
    eletroterapia, luz pulsada, microagulhamento, medical san, smart derma pen, ultrasom, tecare, ethernia, tekah, epilação a laser, crioderms, radiofrequencia,
    hertix, criofrequencia, biosept, locações de equipamentos de estetica, equipamentos para locaçao, eccos, cosmeticos organicos, dermocosmeticos,
    dermocosmeticos organicos," >

    <meta http-equiv=”Content-Type” content=”text/html; charset=iso-8859-1″>


    @if (LojaCorp.Control.Company._Company.StatusGTM)
    {<!-- GoogleTagManager Common -->
        @Html.Action("GoogleTagManager", "Common", new { origin = ViewContext.RouteData.Values["controller"], action = ViewContext.RouteData.Values["action"] })
        <!-- End GoogleTagManager Common -->
    }


    <!-- ~/Views/Shared/Common/_Header.cshtml -->
    @Html.Partial("Common/_Header")
    @RenderSection("CustomSeo", required: false)


    <!-- CSS Styles -->
    @Html.Partial("Common/_CommonCss")
    @RenderSection("custom_css", required: false)


    <!-- Scripts -->
    @Html.Partial("Common/_CommonJs")
    @RenderSection("custom_js", required: false)

    <!-- Google Analytics -->
    @{
        if (!string.IsNullOrEmpty(LojaCorp.Control.Company._Company.IdAnalytics) && !LojaCorp.Control.Company._Company.StatusGTM)
        {
            <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', '@LojaCorp.Control.Company._Company.IdAnalytics', 'auto');
            ga('require', 'displayfeatures');
            ga('send', 'pageview');
            </script>
        }
    }
    <!-- End Google Analytics -->
    
</head>
<body>
    @if (LojaCorp.Control.Company._Company.StatusGTM)
    {
        <!-- Google Tag Manager (noscript) -->
        <noscript>
            <iframe src="https://www.googletagmanager.com/ns.html?id=@LojaCorp.Control.Company._Company.GTM"
                    height="0" width="0" style="display:none;visibility:hidden"></iframe>
        </noscript>
        <!-- End Google Tag Manager (noscript) -->
    }

    <!-- ~/Views/Shared/Common/_Cart.cshtml -->
    @Html.Action("GetCartBySession", "Checkout")

    <div class="pusher">
        <!-- ~/Views/Shared/Common/_TopBar.cshtml -->
        @Html.Partial("Common/_TopBar")

        <!-- ~/Views/Shared/Common/_Head.cshtml -->
        @Html.Partial("Common/_Head")

        <!-- ~/Views/Shared/Common/_Menu.cshtml -->
        @Html.Action("Menu", "Home")

        @RenderBody()

        <!-- ~/Views/Shared/Common/_Footer.cshtml -->
        @Html.Partial("Common/_Footer")
    </div>

    @RenderSection("remarketing", required: false)


@if (Session["UserAuthenticated"] == null && Session["Seller"] == null)
{
    <div class="ui modal login">
        <i class="close icon"></i>
        <div class="header ui center aligned segment">Faça login ou cadastre-se para continuar</div>
        <div class="description">
            @Html.Action("LoginPartial", "Customer", new { _class = "modal-login", returnURL = "/Customer/AutoAddProductInWishListBeforeLogin?redirectToPage=/Home/Index" })
        </div>
    </div>
}

</body>
</html>
